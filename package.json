{
  "name": "NEXT-REDUX-TOOLKIT",
  "description": "NEXT REDUX TOOLKIT",
  "author": "AlexSmith716",
  "private": true,
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml",
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "type-check": "tsc",
    "lint": "next lint",
    "start-custom-server": "NODE_ENV=development node server.js",
    "cypress:open:e2e:preprocessor": "next dev & cypress open --config-file cypress.config.ts --e2e --browser electron",
    "cypress:open:e2e:nodeEvents:electron": "NODE_ENV=development node server.js & cypress open --config-file cypress.nodeEvents.config.ts --e2e --browser electron",
    "cypress:run:e2e:nodeEvents:headless": "cypress run --config-file cypress.nodeEvents.config.ts",
    "test": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.328.0",
    "@aws-sdk/client-s3": "^3.328.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.328.0",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "next": "^13.4.1",
    "next-redux-wrapper": "^9.0.0-rc.2",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "styled-components": "^5.3.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "babel-plugin-styled-components": "^2.1.1",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "eslint-plugin-testing-library": "^5.10.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
